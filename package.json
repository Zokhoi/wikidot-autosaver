{
  "name": "shuen",
  "productName": "Shuen",
  "description": "Electron application for offline editing and syncing of wikidot pages",
  "scripts": {
    "build": "concurrently \"pnpm build:main\" \"pnpm build:renderer\" \"pnpm clonewj\"",
    "build:main": "cross-env NODE_ENV=production webpack --config ./.erb/configs/webpack.config.main.prod.babel.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./.erb/configs/webpack.config.renderer.prod.babel.js",
    "clonewj": "node .erb/scripts/GitPackageClone.js",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "package": "rm -rf src/dist && pnpm build && electron-builder build --publish never",
    "postinstall": "node .erb/scripts/CheckNativeDep.js && electron-builder install-app-deps && pnpm cross-env NODE_ENV=development webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.babel.js",
    "start": "node -r @babel/register ./.erb/scripts/CheckPortInUse.js && pnpm start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ./.erb/scripts/BabelRegister ./src/main.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config ./.erb/configs/webpack.config.renderer.dev.babel.js",
    "test": "jest",
    "winpackage": "rmdir /Q /S \"src/dist\" & pnpm build && electron-builder build --publish never"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "build": {
    "productName": "Shuen",
    "appId": "com.zokhoi.shuen",
    "files": [
      "dist/",
      "node_modules/",
      "index.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "afterSign": ".erb/scripts/Notarize.js",
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "nsis": {
      "perMachine": false,
      "oneClick": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "fileAssociations": [
        {
          "ext": "ftml",
          "name": "FTML File",
          "role": "Editor"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "src",
      "buildResources": "assets",
      "output": "release"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "Zokhoi",
      "repo": "wikidot-autosaver"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zokhoi/wikidot-autosaver.git"
  },
  "author": {
    "name": "Zokhoi",
    "email": "zokhoi@zokhoi.com",
    "url": "https://zokhoi.com"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zokhoi/wikidot-autosaver/issues"
  },
  "keywords": [
    "wikidot",
    "autosave",
    "shuen"
  ],
  "homepage": "https://github.com/Zokhoi/wikidot-autosaver#readme",
  "jest": {
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/node_modules"
    ],
    "setupFiles": [
      "./.erb/scripts/CheckBuildsExist.js"
    ]
  },
